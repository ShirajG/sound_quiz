<!DOCTYPE html>
<html>
<head>
  <title>Trackmastertest</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Fira+Sans:300|Lato:100,400,900' rel='stylesheet' type='text/css'>
</head>
<body>
  <%= render "/navbar" %>

  <div class="container">


    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg %>
    <% end %>

    <%= yield %>
  </div>

</body>
</html>
